{
  "version": 2,
  "framework": null,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/index.php",
      "use": "vercel-php@0.6.0"
    }
  ],
  "routes": [
    {
      "src": "/(css|js|images|fonts|storage)/(.*)$",
      "dest": "/public/$1/$2"
    },
    {
      "src": "/build/(.*)$",
      "dest": "/public/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "env": {
    "APP_NAME": "CommServe",
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "APP_URL": "https://commserve-laravel.vercel.app",
    "SESSION_DRIVER": "cookie",
    "SESSION_LIFETIME": "120",
    "CACHE_DRIVER": "array",
    "QUEUE_CONNECTION": "sync",
    "LOG_CHANNEL": "stderr",
    "LOG_LEVEL": "error"
  },
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  }
}
